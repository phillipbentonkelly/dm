.widget-wrapper .section-header {
	margin-bottom: 5px;
	font-size: 21px;
	line-height: 22px
}

.widget-wrapper .section-header h3 {
	font-size: 21px;
	display: block;
	line-height: 22px;
	font-family: Arial, "Helvetica Neue", Helvetica, sans-serif;
	color: #272727;
	font-weight: bold
}

.widget-wrapper .section-header h3 a {
	padding-right: 16px;
	color: #272727;
	text-decoration: none;
	border: 0
}

.widget-wrapper .widget:first-child {
	margin-top: 0
}

.widget-wrapper .big-news-top {
	margin-bottom: 10px;
	padding-bottom: 10px;
	border-bottom: 1px dotted #3e5f9d
}

.widget-wrapper .last {
	border: none !important
}

.widget-wrapper[data-vr-zone="Big News"] {
	border-bottom: 1px dotted #3e5f9d;
	padding-bottom: 10px
}

.widget-wrapper[data-vr-zone="Big News"] .last {
	border-left: 1px dotted #3e5f9d !important
}

.widget {
	overflow: hidden;
	font-size: 12px;
	line-height: 18px;
	clear: both
}

.widget .overline {
	margin: 0px 0px 5px;
	padding: 0px
}

.widget .overline h3 {
	font-weight: bold;
	font-size: 21px;
	line-height: 23px;
	color: #272727;
	letter-spacing: -.04em;
	margin: 0px !important;
	padding: 0px !important
}

.widget .overline h3 a {
	color: #272727
}

.widget .overline h3 a:hover {
	border-bottom: none
}

.widget .overline .sub-overline {
	font-family: Georgia, "Times New Roman", Times, serif;
	padding-top: 5px;
	display: block
}

.widget span.timestamp {
	font-size: 10px;
	font-family: Arial, Helvetica;
	font-weight: normal;
	color: #666;
	padding-left: 5px;
	position: relative
}

.widget span.timestamp:before {
	content: "( "
}

.widget span.timestamp:after {
	content: " )"
}

div.row div.widget:not(:first-child) {
	margin-top: 1em
}

div.row div.widget-wrapper div.section-header+div.widget {
	margin-top: 0em
}

.row ul.link-list {
	clear: both;
	list-style: outside disc;
	margin-top: 10px;
	margin-left: 15px
}

.row ul.link-list li {
	color: #999;
	line-height: 14px;
	padding-bottom: 4px
}

.row ul.link-list li span {
	font-family: Arial, Helvetica, Verdana, sans-serif;
	font-weight: bold;
	text-transform: uppercase;
	color: #666;
	font-size: 11px;
	line-height: 12px
}

.row ul.link-list li.strong a {
	font-weight: bold
}

.row ul.link-list li.pipes ul li {
	display: inline
}

.row ul.link-list li.pipes ul li a {
	font-weight: bold
}

.row ul.link-list li.pipes ul li:before {
	content: "|";
	padding-right: 5px
}

.row ul.link-list li.pipes ul li:first-child:before {
	content: none
}

.row ul.link-list.sans-bullets {
	list-style: none
}

.lead-in {
	text-align: right;
	padding-top: 10px;
	text-transform: uppercase;
	font-size: 10px;
	line-height: 12px;
	font-family: Arial, "Helvetica Neue", Helvetica, sans-serif;
	font-weight: bold;
	color: #666
}

.lead-out {
	margin: 0px 0px 5px 0px;
	text-align: right;
	text-transform: uppercase;
	font-size: 10px;
	line-height: 12px;
	font-family: Arial, "Helvetica Neue", Helvetica, sans-serif
}

.lead-out:after {
	font-size: 24px;
	vertical-align: bottom;
	padding-left: 3px;
	color: #2851a2;
	content: '\2023'
}

.tease {
	display: block
}

.tease div.overline h3 {
	font-size: 10px;
	line-height: 11px;
	margin-bottom: 2px;
	color: #272727;
	text-transform: uppercase;
	font-family: Arial, "Helvetica Neue", Helvetica, sans-serif;
	font-weight: normal;
	margin: 0px;
	padding: 0px
}

.tease a.image-link {
	text-decoration: none;
	border-bottom: none
}

.tease.list .overline h3 {
	font-size: 21px;
	line-height: 23px;
	font-weight: bold;
	text-transform: none
}

.tease.list .overline h3 a {
	color: #272727
}

.tease.list .overline h3 a:hover {
	color: #272727
}

.tease.list ul li {
	margin: 5px;
	padding-bottom: 5px;
	vertical-align: top;
	display: inline-block;
	width: 100%;
	overflow: hidden;
	border-bottom: 1px dotted #3e5f9d
}

.tease.list ul li .image {
	border-top: none;
	float: left;
	display: inline;
	margin: 0 .5em 0 0
}

.tease.list ul li .image a.image-link {
	text-decoration: none;
	border-bottom: none
}

.tease.list ul li img.inline-image {
	margin-right: 10px
}

.tease.blog .overline a {
	text-transform: uppercase;
	color: #272727;
	font-size: 10px;
	line-height: 11px
}

.tease.blog h2 {
	padding: 0px;
	margin: 0px
}

.tease.image-left .image {
	border-top: none;
	float: left;
	display: inline;
	margin: 0 .5em 0 0
}

.tease.image-full .image {
	border-top: none;
	display: block;
	margin: 0
}

.tease h3 {
	font-size: 16px;
	font-weight: bold;
	line-height: 19px
}

.tease h3.big-headline {
	font-size: 17px;
	line-height: 19px
}

.tease h3.bigger-headline {
	font-size: 24px;
	line-height: 25px
}

.tease h3.biggest-headline {
	font-size: 30px;
	line-height: 33px
}

.tease h3 .subject {
	font-size: 11px;
	color: #666;
	text-transform: uppercase
}

.tease p {
	font-family: Georgia, "Times New Roman", Times, serif;
	margin: 0;
	padding: 3px 0 0 0;
	line-height: 16px
}

.tease .simple p {
	font-family: Arial, "Helvetica Neue", Helvetica, sans-serif
}

.tease .source {
	font-family: Arial, "Helvetica Neue", Helvetica, sans-serif;
	font-size: 10px;
	color: #464646;
	font-weight: normal
}

