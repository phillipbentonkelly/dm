.widget .blue-box {
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background: #EBF4F9;
	border: 1px solid #BEC8FF;
	padding: 10px 10px;
	position: relative;
	overflow: auto
}

.widget .blue-box h3 {
	font-size: 17px;
	display: block;
	font-weight: bold;
	line-height: 22px
}

.widget .blue-box .content {
	padding: 10px;
	background: white;
	margin-top: 6px;
	border-top: solid 1px #B7DCFF
}

h4.subhead {
	font-weight: bold;
	text-transform: uppercase;
	line-height: 14px;
	padding-bottom: 7px;
	font-size: 12px
}

h4.subhead a:after {
	padding-left: 3px;
	color: #272727;
	content: '\00BB'
}

ul.bulleted {
	list-style: disc;
	color: #999;
	padding: 0 0 3px 15px;
	line-height: 16px
}

ul.bulleted.black {
	color: #000
}

ul.bulleted li {
	line-height: 14px;
	padding-bottom: 4px
}

ul.forum li:first-child {
	font-weight: bold
}

span.bold {
	font-size: 14px;
	font-weight: bold
}

.widget.inside-bcom ul.inside-bcom-list {
	margin: 0;
	padding: 0;
	list-style-type: none;
	background-color: #EBF4FB;
	overflow: hidden
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item
	{
	float: left;
	width: 199px;
	overflow: hidden;
	list-style-type: none;
	margin: 0;
	padding: 10px 0
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item a
	{
	display: block
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item a .lead-in
	{
	text-align: left;
	font-size: 11px;
	padding-bottom: 3px;
	padding-top: 0px !important
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item a .inside-caption
	{
	color: #3E5F9D;
	font-size: 12px;
	line-height: 14px;
	width: 179px;
	font-weight: bold
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item:hover
	{
	background: #b7dcff;
	color: #2851A2
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item:hover a
	{
	border-bottom: none
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item:hover .lead-in
	{
	color: #333
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.inside-item:hover .inside-caption
	{
	color: #002878
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.plus-list {
	overflow: hidden
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.plus-list .lead-in
	{
	text-align: left;
	font-size: 11px;
	padding-top: 0px !important;
	padding-bottom: 3px
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.plus-list ul.link-list
	{
	margin-top: 0;
	line-height: 13px;
	overflow: hidden;
	margin: 0;
	padding: 0;
	list-style-position: outside
}

.widget.inside-bcom.inside-horizontal ul.inside-bcom-list li.plus-list ul.link-list li
	{
	width: auto;
	line-height: 12px;
	margin-left: 15px;
	padding-bottom: 2px;
	float: none;
	list-style-type: disc
}

.widget.inside-bcom.vertical-wide .component-header h3 {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 19px/16px;
	color: #000
}

.widget.inside-bcom.vertical-wide .component-header h3:after {
	content: ' '
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list {
	margin: 0px;
	padding: 0px;
	list-style-type: none;
	background: #fff
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item {
	overflow: hidden;
	margin-top: 1em;
	padding-bottom: 1em;
	border-bottom: 1px solid #C9C9C9
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item:first-child
	{
	margin-top: 0
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a {
	text-decoration: none
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a .inside-image
	{
	float: left;
	display: block;
	margin-right: 1em
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a .inside-image img
	{
	padding-top: 3px;
	display: block
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a .inside-content
	{
	margin: 1em 0px 0px 0px;
	text-align: left;
	font-family: arial, helvetica, sans-serif
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a .inside-content>div
	{
	margin-top: 5px
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a .inside-content .lead-in
	{
	font-size: 10px;
	line-height: 11px;
	font-weight: normal;
	color: #272727;
	text-transform: uppercase;
	text-align: left;
	padding-top: 0
}

.widget.inside-bcom.vertical-wide ul.inside-bcom-list li.inside-item a .inside-content .inside-caption
	{
	font-size: 16px;
	line-height: 19px;
	color: #45569C;
