#gray-overlay {
    position: fixed;
    display: block;
    background-color: #222;
    z-index: 9999;
    margin: 0;
    padding: 0;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: .9;
    filter: alpha(opacity=90);
    background-position: center center;
    background-repeat: no-repeat;
    background: -webkit-gradient(radial,  center center,  0,  center center,  60,  from(#f5f5f5),  to(#444));
    background: -webkit-radial-gradient(circle,  #f5f5f5,  #444);
    background: -moz-radial-gradient(circle,  #f5f5f5,  #444);
    background: -ms-radial-gradient(circle,  #f5f5f5,  #444);
}
iframe {
    height: 99%;
    width: 99%;
    position: absolute;
    top: 0;
    left: 0;
    padding: 0;
    margin: 0;
    opacity: 0;
    overflow: hidden;
    background-color: transparent;
    z-index: 0;
}
.modal-content-wrapper {
    position: fixed;
    width: 100%;
    top: 10%;
    z-index: 9999;
    text-align: center;
}
a.close-modal, a.close-modal:hover {
    position: fixed;
    width: 100%;
    top: 5%;
    z-index: 10000;
    text-align: center;
    font: 1.4em/1.3em Helvetica, sans-serif;
    color: white;
    text-transform: uppercase;
    border-bottom: none;
}
#externalframe {
    width: 600px;
    height: 500px;
    overflow: hidden;
    margin: auto auto;
    background: white;
}
#emailIssues {
    display: none;
}
div#member-newsletter-subscriptions {
    zoom: 1;
    display: inline-block;
    position: relative;
    margin-top: 10px;
}
div.newsletter-subscribe-item {
    margin-right: 40px;
    padding-top: 15px;
    padding-bottom: 15px;
    border-bottom: 1px dotted #999;
}
.newsletter-name a {
    font-size: 12px;
}
.newsletter-description {
    display: block;
    height: 110px;
    margin-top: 5px;
    font-size: 13px !important;
}
#container .form-button, #container .form-button:hover {
    width: 160px;
    height: 35px;
    padding: 4px 0 6px 0;
    font-size: 12px;
    margin-top: 1.5em;
    background-color: #2851A2;
    color: #fff;
    border: 0;
    font-weight: bold;
}
.member-update-newsletters {
    margin-left: 0;
    margin-top: .25em;
    clear: left;
    float: left;
}
.newsletter-action {
    font-size: 14px;
    font-weight: bold;
}
.newsletter-unsubscribe-all {
    border: 1px dotted #999;
    margin: 25px 270px 15px 145px;
}
.newsletter-unsubscribe-all-contents {
    margin: 10px;
}
div#member-newsletter-subscriptions .form-button, div#member-newsletter-subscriptions .form-button:hover {
    float: none;
}
.newsletter-unsubscribe {
    padding-top: 20px;
}
.section-header {
    padding-bottom: 3px;
    border-bottom: 3px solid #00008b;
}
.newsletter-action-description {
    padding-left: 20px;
    font-size: 15px;
    font-weight: bold;
}
.newsletter-subscribe {
    margin-top: 40px;
}
.newsletter-subscribe .newsletter-action {
    padding-left: 10px;
}
.newsletter-subscribe .newsletter-action-description {
    padding-left: 28px;
}
