body {
	padding: 0;
	margin: 0;
	font-family: helvetica, arial, sans-serif;
	font-size: 16px;
	color: #222;
}
a {
	text-decoration: none;
}
header {
	padding: .5em 20px;
	background: #0d5799;
	font-size: .9em;
	color: #eee;
}
header:before, header:after { content: " "; display: table; }
header:after { clear: both; }
header { *zoom: 1; }
header a {
	color: #fff;
}
.user-info {
	float: right;
}
.greeting {
	position: relative;
	top: -8px;
	margin-right: 10px;
}
.user-avatar {
	width: 30px;
}
form {
	padding: 20px;
}
.col {
	display: inline-block;
	width: 48%;
}
.col:first-child {
	margin-right: 2%;
}
.col:nth-child(2) {
	margin-left: 2%;
}
.inline label{
	display: inline-block;
  margin-right: 10px;
}

label {
	display: block;
	font-weight: bold;
}
select {
	border: 1px solid #aaa;
	padding: 5px 10px;
	width: 100%;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
input[type=text],
textarea {
	width: 100%;
	border: 1px solid #aaa;
	padding: 5px 10px;
	border-radius: 0;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
textarea {
	height: 60px;
}
.image-holder img {
	max-width: 50%;
}
.image-holder {
	padding-top: 10px;
	padding-bottom: 10px;
	background: #eee;
	text-align: center;
}
.streamer-submit-section {
	padding-top: 1em;
}
input[type=submit] {
	background: #2494e3;
	border: none;
	padding: 5px 20px;
	margin-right: 10px;
	cursor: pointer;
	font-family: helvetica, arial, sans-serif;
	color: #fff;
	font-size: 14px;
	text-transform: uppercase;
	letter-spacing: 0.2em;
	border-radius: 0;
	line-height: normal !important;
	-webkit-box-shadow: -1px -1px 0 #1b6faa inset;
}
input[type=submit]:hover {
	background: #399ee5;
	-webkit-transition: all 0.15s ease-in;
	-moz-transition: all 0.15s ease-in;
	-o-transition: all 0.15s ease-in;
	transition: all 0.15s ease-in;
}
.streamer-close {
	border: none;
	background: #2494e3;
	padding: 5px 20px;
	cursor: pointer;
	color: #fff;
	font-size: 14px;
	letter-spacing: 0.2em;
	text-transform: uppercase;
	line-height: normal !important;
	-webkit-box-shadow: -1px -1px 0 #1b6faa inset;
}
.streamer-close:hover {
	background: #399ee5;
	text-decoration: none;
	-webkit-transition: all 0.15s ease-in;
	-moz-transition: all 0.15s ease-in;
	-o-transition: all 0.15s ease-in;
	transition: all 0.15s ease-in;
}
.streamer-form-section {
	margin-bottom: 1em;
}
.streamer-image-preview {
	max-width: 100%;
}
/* Loading Message */
.loading-message {
	font-size: 16px;
	text-align: center;
	font-style: italic;
	margin-top: 20px;
	padding: 40px 0 20px;
	background-repeat: no-repeat;
	background-image: url(data:image/gif;base64,);
	background-position: top center;
}