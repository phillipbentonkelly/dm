// breaking news area scoreboard styles

.breaking-scoreboard-mod {
	background: $black-90;
}
.breaking-scoreboard {
	display: table;
	margin-bottom: .8em;
	width: 100%; 

	ul {
		display: table-cell;
		padding: 15px 10px;
		vertical-align: middle;

		@include mq(max, 675px){ 
			display: table;
			padding: 5px 15px;
		}

		&:first-child {
			padding-left: 15px;
		}
	}
	li { 
		display: table-cell;
		padding: 0px 5px;
		vertical-align: middle;
		@include sans(em(14), 400);
		line-height: 30px;
		color: #fff;
	}
	
	a {
		@include sans(em(14), 700);
		text-transform: uppercase;
	}


	.status-section{
		text-transform: uppercase;
		@include mq(max, 675px){ //325px
			width: 100%;
			padding-bottom: 0px;
			margin-bottom: 10px; 
			border-bottom: 1px solid white;
		}
		li {
			@include mq(max, 675px){ //325px
				display: inline-block;
			}
		}
	}

	.team-section {
		@include mq(max, 675px) {
			width: 35%;
		}
	}

	.more-section {
		@include sans(em(12), 700);
		li {
			float: right;
		}
	}

	.breaking-sb-score {
		@include sans(em(14), 700);
	}
	.breaking-sb-status {
		@include sans(em(14), 700);
	}
}

